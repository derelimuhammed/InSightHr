                        <!-- Contact Form will be functional only on your server. Upload to your server when testing. -->
                        <form method="post" name="contactform" id="contactform">
                            <fieldset>
                                <input name="name" type="text" id="name" placeholder="İSİM" required/>
                                <input name="email" type="text" id="email" placeholder="EMAİL" required />
                                <input name="subject" type="text" id="subject" placeholder="KONU" required />
                            </fieldset>
                            <fieldset>
                                <textarea name="comments" cols="40" rows="3" id="comments" placeholder="MESAJ" required></textarea>
                            </fieldset>
                            <input type="submit" onclick="showSweetAlert()" class="submit" id="submit" value="GÖNDER" />
                        </form>
                    </div>

                    <div class="col-md-6 wow fadeInLeft">
                        <img src="/assets/img/InSight-Contact.jpg" class="img-responsive" alt="" />
